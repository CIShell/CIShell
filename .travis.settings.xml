<settings xmlns="http://maven.apache.org/SETTINGS/1.1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd">
	<servers>
		<server>
			<username>${env.CI_DEPLOY_USERNAME}</username>
			<password>${env.CI_DEPLOY_PASSWORD}</password>
			<id>cishell-release</id>
		</server>
		<server>
			<username>${env.CI_DEPLOY_USERNAME}</username>
			<password>${env.CI_DEPLOY_PASSWORD}</password>
			<id>cishell-snapshot</id>
		</server>
	</servers>
	<profiles>
		<profile>
			<repositories>
				<repository>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
					<id>cishell-release</id>
					<name>cishell-release</name>
					<url>https://cishelltest.jfrog.io/cishelltest/release-local</url>
				</repository>
				<repository>
					<snapshots>
						<enabled>true</enabled>
					</snapshots>
					<id>cishell-snapshot</id>
					<name>cishell-snapshot</name>
					<url>https://cishelltest.jfrog.io/cishelltest/snapshot-local</url>
				</repository>
			</repositories>
			<id>artifactory</id>
		</profile>
	</profiles>
	<activeProfiles>
		<activeProfile>artifactory</activeProfile>
	</activeProfiles>
</settings>